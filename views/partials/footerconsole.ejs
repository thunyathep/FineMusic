<body>
  <div class="container-fluid fixed-bottom console">
    <div class="row">


      <div class="col-3">
        <div class="d-flex">
          <div class="flex-shrink-0 mt-2 m-2 play-Image">
            <img
              src="https://t3.ftcdn.net/jpg/04/54/66/12/360_F_454661277_NtQYM8oJq2wOzY1X9Y81FlFa06DVipVD.jpg"
              style="width:90px;height:90px" class="align-sef-center mr-3 image-console">
          </div>
          <div class="flex-grow-1 ms-3 mt-2 text-white">
            <h4 class="mt-0 name-console"></h4>
            <p class="artist-console"></p>
          </div>
        </div>
      </div>



      <div class="col-6">
        <div class="play-icon text-white d-flex align-items-center justify-content-center">
          <!-- <div class="m-3"><i class="fa-solid fa-backward-step prev"></i></div> -->
          <div class="m-3"><i class="fa-solid fa-circle-play" id="playmusic-console"></i></div>
          <!-- <div class="m-3"><i class="fa-solid fa-forward-step next"></i></div> -->
        </div>
        <div class="progress-block mt-0 text-white d-flex justify-content-between align-items-center">
          <!-- <div class="m-2">0:00</div> -->
          <div class="progress music-progress w-100">
            <input class="progress-bar bg-success w-100" role="progressbar" type="range"></input>
          </div>
          <!-- <div class="m-2">4:00</div> -->
        </div>
      </div>



      <div class="col-3 text-white d-flex d-inline-flex text-right flex-row-reverse align-items-center">
        <div class="progress ">
          <input class="progress-bar bg-success volume-progress" role="progressbar" type="range" min="0" max="1" step="0.05"></input>
        </div>
        <div class="m-2"><i class="fa-solid fa-volume-low"></i></div>
        <div class="m-2"><i class="fa-solid fa-list"></i></div>
      </div>

    </div>
  </div>
</body>